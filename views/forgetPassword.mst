{{>header}}

<h1>Mude a sua senha</h1>

<form method="POST">
    <label>
        Nova Senha:
        <input type="password" name="password"/>
    </label>
    <br>
    <label>
        Comfirmar Nova Senha:
        <input type="password" name="password-confirm"/>
    </label>
    <br>
    <input type="submit" name="Salvar"/>
</form>
