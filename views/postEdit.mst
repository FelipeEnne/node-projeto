{{>header}}

<h2>Editar Post</h2>

<form method="POST">
    <label>
        Titulo:
        <input type="text" name="title" value={{post.title}}/>
    </label>
    <br>
    <label>
        Corpo:
        <textarea name="body">{{post.body}}</textarea>
    </label>
    <br>
    <label>
        Tags: (separar por v√≠rgula)
        <input type="text" name="tags" value={{post.tags}} />
    </label>
    <br>
    <input type="submit" name="Salvar"/>
</form>